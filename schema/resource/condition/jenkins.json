{
    "$id": "/resource/condition/jenkins",
    "type": "object",
    "description": "Test if a specific Jenkins version exist",
    "unevaluatedProperties": false,
    "properties": {
        "release": {
            "description": "Define Jenkins release type",
            "enum": [
                "stable",
                "weekly"
            ]
        },
        "version": {
            "type": "string",
            "description": "Specify Jenkins version which should exist"
        }
    }
}
