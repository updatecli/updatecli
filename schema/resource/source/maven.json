{
    "$id": "/resource/source/maven",
    "type": "object",
    "description": "Get latest Maven artifact version from maven repository",
    "unevaluatedProperties": false,
    "required": [
        "artifactID",
        "groupID",
        "repository",
        "url"
    ],
    "properties": {
        "url": {
            "description": "Define Maven repository url",
            "type": "string"
        },
        "artifactID": {
            "description": "Define Maven artifact ID",
            "type": "string"
        },
        "groupID": {
            "description": "Define Maven groupID",
            "type": "string"
        },
        "repository": {
            "description": "Define Maven repository name",
            "type": "string"
        }
    } 
}

